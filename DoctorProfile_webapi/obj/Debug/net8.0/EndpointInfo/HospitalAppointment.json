  "components": {
    "schemas": {
      "Appointment": {
        "required": [
          "appointmentDate",
          "appointmentTime",
          "status"
        ],
        "type": "object",
        "properties": {
          "appointmentId": {
            "type": "integer",
            "format": "int32"
          },
          "patientId": {
            "type": "integer",
            "format": "int32"
          },
          "doctorId": {
            "type": "integer",
            "format": "int32"
          },
          "availabilityId": {
            "type": "integer",
            "format": "int32"
          },
          "appointmentDate": {
            "type": "string",
            "format": "date"
          },
          "appointmentTime": {
            "type": "string",
            "format": "date-span"
          },
          "status": {
            "$ref": "#/components/schemas/AppointmentStatus"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "concern": {
            "maxLength": 255,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "AppointmentStatus": {
        "enum": [
          0,
          1,
          2,
          3
        ],
        "type": "integer",
        "format": "int32"
      },
      "Doctor": {
        "required": [
          "experienceYears",
          "gender",
          "name",
          "qualification",
          "specialization",
          "userId"
        ],
        "type": "object",
        "properties": {
          "doctorId": {
            "type": "integer",
            "format": "int32"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "maxLength": 100,
            "minLength": 3,
            "type": "string"
          },
          "phone": {
            "maxLength": 10,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "specialization": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "experienceYears": {
            "type": "integer",
